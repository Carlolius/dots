# Aliases #

alias cls="clear"
alias ll="ls -lisa"
alias home="cd ~"
alias myip="curl http://ipecho.net/plain; echo && hostname -i"

alias copy="xclip -o | xclip -selection clipboard -i"

alias musica="spotifyd && spt"
# Aliases of my functions
